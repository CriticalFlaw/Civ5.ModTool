<Window x:Class="CivModTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:x2="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="Civilization V - Mod Tool"
        Height="550" Width="1000"
        FontFamily="Century Gothic"
        SizeToContent="Manual"
        ResizeMode="CanMinimize">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Resources/Style/background.jpg" />
        </Grid.Background>
        <TabControl x:Name="TabControls" Margin="10,10,10,10" Background="Transparent">
            <TabItem Header="Civilization">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="103*" />
                        <ColumnDefinition Width="865*" />
                    </Grid.ColumnDefinitions>
                    <Grid.Background>
                        <SolidColorBrush Color="#191919" Opacity="0.85" />
                    </Grid.Background>
                    <Label Content="Civilization:" HorizontalAlignment="Left" Margin="10,10,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Adjective:" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" />
                    <Label Content="Description:" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" />
                    <Label Content="Short Description:" HorizontalAlignment="Left" Margin="10,100,0,0"
                           VerticalAlignment="Top" Grid.ColumnSpan="2" />
                    <Label Content="Civilopedia:" HorizontalAlignment="Left" Margin="10,130,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Art Style:" HorizontalAlignment="Left" Margin="10,160,0,0" VerticalAlignment="Top" />
                    <Label Content="Portrait Index:" HorizontalAlignment="Left" Margin="175,100,0,0"
                           VerticalAlignment="Top" Grid.Column="1" />
                    <Label Content="Soundtrack:" HorizontalAlignment="Left" Margin="10,310,0,0" VerticalAlignment="Top" />
                    <Label Content="Dawn of Man Quote:" HorizontalAlignment="Left" Margin="175,130,0,0"
                           VerticalAlignment="Top" Grid.Column="1" />
                    <Label Content="Primary Color:" HorizontalAlignment="Left" Margin="175,10,0,0"
                           VerticalAlignment="Top" Grid.Column="1" />
                    <Label Content="Secondary Color:" HorizontalAlignment="Left" Margin="175,40,0,0"
                           VerticalAlignment="Top" Grid.Column="1" />
                    <Label Content="Text Color:" HorizontalAlignment="Left" Margin="175,70,0,0" VerticalAlignment="Top"
                           Grid.Column="1" />
                    <Label Content="Free Building:" HorizontalAlignment="Left" Margin="10,190,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Free Unit:" HorizontalAlignment="Left" Margin="10,220,0,0" VerticalAlignment="Top" />
                    <Label Content="Free Technology:" HorizontalAlignment="Left" Margin="10,250,0,0"
                           VerticalAlignment="Top" Grid.ColumnSpan="2" />
                    <Label Content="Religion:" HorizontalAlignment="Left" Margin="10,280,0,0" VerticalAlignment="Top" />
                    <Label Content="City Names:" HorizontalAlignment="Left" Margin="435,10,0,0" VerticalAlignment="Top"
                           Grid.Column="1" />
                    <Label Content="Spy Names:" HorizontalAlignment="Left" Margin="660,10,0,0" VerticalAlignment="Top"
                           Grid.Column="1" />

                    <TextBox x:Name="tbType" HorizontalAlignment="Left" Margin="26.6,10,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" CharacterCasing="Upper" Grid.Column="1" />
                    <TextBox HorizontalAlignment="Left" Margin="26.6,40,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" Grid.Column="1" />
                    <TextBox HorizontalAlignment="Left" Margin="26.6,70,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" Grid.Column="1" />
                    <TextBox HorizontalAlignment="Left" Margin="26.6,100,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" Grid.Column="1" />
                    <TextBox HorizontalAlignment="Left" Margin="26.6,130,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" Grid.Column="1" />
                    <ComboBox x:Name="cbArtStyle" HorizontalAlignment="Left" Margin="26.6,160,0,0"
                              VerticalAlignment="Top" Width="120" SelectedIndex="1" Grid.Column="1" />
                    <x2:IntegerUpDown x:Name="intCivPortraitIndex" Margin="295,100,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="0" Increment="1" Minimum="0"
                                      Maximum="4" Width="120" AllowTextInput="False" Grid.Column="1" />
                    <ComboBox x:Name="cbSoundtrack" HorizontalAlignment="Left" Margin="27,310,0,0"
                              VerticalAlignment="Top" Width="120" SelectedIndex="0" Grid.Column="1" />
                    <TextBox HorizontalAlignment="Left" Margin="175,160,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="240" Grid.Column="1" Height="171" />

                    <x2:ColorPicker
                        x:Name="cpPrimaryColor"
                        Width="120"
                        Margin="294.8,10,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        ColorMode="ColorCanvas"
                        ShowRecentColors="True"
                        DisplayColorAndName="True"
                        ShowDropDownButton="False"
                        SelectedColor="Black" Grid.Column="1" />
                    <x2:ColorPicker
                        x:Name="cpSecondaryColor"
                        Width="120"
                        Margin="294.8,40,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        ColorMode="ColorCanvas"
                        ShowRecentColors="True"
                        DisplayColorAndName="True"
                        ShowDropDownButton="False"
                        SelectedColor="Yellow" Grid.Column="1" />
                    <x2:ColorPicker
                        x:Name="cpTextColor"
                        Width="120"
                        Margin="294.8,70,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        ColorMode="ColorCanvas"
                        ShowRecentColors="True"
                        DisplayColorAndName="True"
                        ShowDropDownButton="False"
                        SelectedColor="White" Grid.Column="1" />

                    <ComboBox x:Name="cbFreeBuilding" HorizontalAlignment="Left" Margin="27,190,0,0"
                              VerticalAlignment="Top" Width="120" SelectedIndex="64" Grid.Column="1" />
                    <ComboBox x:Name="cbFreeUnit" HorizontalAlignment="Left" Margin="27,220,0,0"
                              VerticalAlignment="Top" Width="120" SelectedIndex="60" Grid.Column="1" />
                    <ComboBox x:Name="cbFreeTech" HorizontalAlignment="Left" Margin="27,250,0,0"
                              VerticalAlignment="Top" Width="120" SelectedIndex="0" Grid.Column="1" />
                    <ComboBox x:Name="cbReligion" HorizontalAlignment="Left" Margin="27,280,0,0"
                              VerticalAlignment="Top" Width="120" SelectedIndex="0" Grid.Column="1" />

                    <ListBox x:Name="lbCityNames" HorizontalAlignment="Left" Height="290" Margin="435,41,0,0"
                             VerticalAlignment="Top" Width="125" Grid.Column="1" />
                    <Button x:Name="bAddCity" Content="Add" HorizontalAlignment="Left" Margin="565,51,0,0"
                            VerticalAlignment="Top" Width="75" Click="BAddCity_OnClick" Grid.Column="1" />
                    <Button x:Name="bRemoveCity" Content="Remove" HorizontalAlignment="Left" Margin="565,81,0,0"
                            VerticalAlignment="Top" Width="75" Click="BRemoveCity_OnClick" Grid.Column="1" />
                    <Button x:Name="bClearCity" Content="Clear" HorizontalAlignment="Left" Margin="565,111,0,0"
                            VerticalAlignment="Top" Width="75" Click="BClearCity_OnClick" Grid.Column="1" />

                    <ListBox x:Name="lbSpyNames" HorizontalAlignment="Left" Height="290" Margin="645,41,0,0"
                             VerticalAlignment="Top" Width="125" Grid.Column="1" />
                    <Button x:Name="bAddSpy" Content="Add" HorizontalAlignment="Left" Margin="775,51,0,0"
                            VerticalAlignment="Top" Width="75" Click="bAddSpy_Click" Grid.Column="1" />
                    <Button x:Name="bRemoveSpy" Content="Remove" HorizontalAlignment="Left" Margin="775,81,0,0"
                            VerticalAlignment="Top" Width="75" Click="bRemoveSpy_Click" Grid.Column="1" />
                    <Button x:Name="bClearSpy" Content="Clear" HorizontalAlignment="Left" Margin="775,111,0,0"
                            VerticalAlignment="Top" Width="75" Click="bClearSpy_Click" Grid.Column="1" />

                    <Button x:Name="btnGenerateXML" Content="Generate" HorizontalAlignment="Left"
                            Margin="10,340,0,0" VerticalAlignment="Top" Height="25" Width="240"
                            Click="btnGenerateXML_Click" Grid.ColumnSpan="2" />
                </Grid>
            </TabItem>
            <TabItem Header="Leader">
                <Grid>
                    <Grid.Background>
                        <SolidColorBrush Color="#191919" Opacity="0.85" />
                    </Grid.Background>
                    <Label Content="Leader:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" />
                    <Label Content="Description:" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" />
                    <Label Content="Civilopedia:" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" />
                    <Label Content="Civilopedia Tag:" HorizontalAlignment="Left" Margin="10,100,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Portrait Index:" HorizontalAlignment="Left" Margin="250,220,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Competitive - Victory:" HorizontalAlignment="Left" Margin="10,250,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Competitive - Wonder:" HorizontalAlignment="Left" Margin="10,280,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Competitive - Minor:" HorizontalAlignment="Left" Margin="10,310,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Boldness:" HorizontalAlignment="Left" Margin="250,280,0,0" VerticalAlignment="Top" />
                    <Label Content="Diplomacy Balance:" HorizontalAlignment="Left" Margin="250,250,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Warmonger Hate:" HorizontalAlignment="Left" Margin="10,340,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Work vs. Willingness:" HorizontalAlignment="Left" Margin="10,370,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Work With Willingness:" HorizontalAlignment="Left" Margin="10,400,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Denounce Willingness:" HorizontalAlignment="Left" Margin="10,430,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Loyalty:" HorizontalAlignment="Left" Margin="250,310,0,0" VerticalAlignment="Top" />
                    <Label Content="Neediness:" HorizontalAlignment="Left" Margin="250,340,0,0" VerticalAlignment="Top" />
                    <Label Content="Forgiveness:" HorizontalAlignment="Left" Margin="250,370,0,0" VerticalAlignment="Top" />
                    <Label Content="Chattiness:" HorizontalAlignment="Left" Margin="250,400,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Meanness:" HorizontalAlignment="Left" Margin="250,430,0,0" VerticalAlignment="Top" />
                    <TextBox x:Name="tbLeaderType" HorizontalAlignment="Left" Margin="125,10,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" CharacterCasing="Upper" />
                    <TextBox HorizontalAlignment="Left" Margin="125,40,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" />
                    <TextBox HorizontalAlignment="Left" Margin="125,70,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" />
                    <TextBox HorizontalAlignment="Left" Margin="125,100,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" />
                    <x2:IntegerUpDown x:Name="intLeaderPortraitIndex" Margin="389,220,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="0"
                                      Maximum="4" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intCompVictory" Margin="180,250,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intCompWonder" Margin="180,280,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intCompMinor" Margin="180,310,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intBoldness" Margin="389,280,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intDiploBalance" Margin="389,250,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intWarmongerHate" Margin="180,340,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intWorkAgainstWill" Margin="180,370,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intWorkWithWill" Margin="180,400,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intDenounceWill" Margin="180,430,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intLoyalty" Margin="389,310,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intNeediness" Margin="389,340,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intForgiveness" Margin="389,370,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intChattiness" Margin="389,400,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intMeanness" Margin="389,430,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <Label Content="Major Approach Bias:" HorizontalAlignment="Left" Margin="450,20,0,0" VerticalAlignment="Top" />
                    <Label Content="Minor Approach Bias:" HorizontalAlignment="Left" Margin="450,170,0,0" VerticalAlignment="Top" />
                    <Label Content="Flavors:" HorizontalAlignment="Left" Margin="450,320,0,0" VerticalAlignment="Top" />
                    <ComboBox x:Name="cbMajorBias" HorizontalAlignment="Left" Margin="600,20,0,0" VerticalAlignment="Top" Width="150" SelectedIndex="1" />
                    <ComboBox x:Name="cbMinorBias" HorizontalAlignment="Left" Margin="600,170,0,0" VerticalAlignment="Top" Width="150" SelectedIndex="1" />
                    <ComboBox x:Name="cbFlavors" HorizontalAlignment="Left" Margin="600,320,0,0" VerticalAlignment="Top" Width="150" SelectedIndex="1" />

                    <Label Content="Count:" HorizontalAlignment="Left" Margin="760,20,0,0" VerticalAlignment="Top" />
                    <Label Content="Count:" HorizontalAlignment="Left" Margin="760,170,0,0" VerticalAlignment="Top" />
                    <Label Content="Count:" HorizontalAlignment="Left" Margin="760,320,0,0" VerticalAlignment="Top" />
                    <x2:IntegerUpDown x:Name="intMajorBiasCount" Margin="820,20,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intMinorBiasCount" Margin="820,170,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <x2:IntegerUpDown x:Name="intFlavorCount" Margin="820,320,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="1"
                                      Maximum="10" Width="40" AllowTextInput="False" />
                    <Button x:Name="btnAddMajorBias"  Content="Add" HorizontalAlignment="Left" Margin="865,20,0,0" VerticalAlignment="Top" Width="85" Click="BtnAddMajorBias_OnClick" />
                    <ListBox x:Name="lbMajorBias" HorizontalAlignment="Left" Height="100" Margin="450,50,0,0" VerticalAlignment="Top" Width="500" />
                    <Button x:Name="btnAddMinorBias"  Content="Add" HorizontalAlignment="Left" Margin="865,170,0,0" VerticalAlignment="Top" Width="85" Click="BtnAddMinorBias_OnClick" />
                    <ListBox x:Name="lbMinorBias" HorizontalAlignment="Left" Height="100" Margin="450,200,0,0" VerticalAlignment="Top" Width="500" />
                    <Button x:Name="btnAddFlavor"  Content="Add" HorizontalAlignment="Left" Margin="865,320,0,0" VerticalAlignment="Top" Width="85" Click="BtnAddFlavor_OnClick" />
                    <ListBox x:Name="lbFlavors" HorizontalAlignment="Left" Height="100" Margin="450,350,0,0" VerticalAlignment="Top" Width="500" />
                    <Button Content="Randomize" HorizontalAlignment="Left" Margin="10,220,0,0" VerticalAlignment="Top" Width="210" Click="ButtonBase_OnClick" />
                </Grid>
            </TabItem>
            <TabItem Header="Trait">
                <Grid>
                    <Grid.Background>
                        <SolidColorBrush Color="#191919" Opacity="0.85" />
                    </Grid.Background>
                    <Label Content="Trait:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" />
                    <TextBox x:Name="tbTraitType" HorizontalAlignment="Left" Margin="125,10,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" CharacterCasing="Upper" />
                </Grid>
            </TabItem>
            <TabItem Header="Buildings">
                <Grid>
                    <Grid.Background>
                        <SolidColorBrush Color="#191919" Opacity="0.85" />
                    </Grid.Background>
                    <Label Content="Building:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" />
                    <Label Content="Building Type:" HorizontalAlignment="Left" Margin="10,40,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Description:" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" />
                    <Label Content="Civilopedia:" HorizontalAlignment="Left" Margin="10,100,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Strategy:" HorizontalAlignment="Left" Margin="10,130,0,0" VerticalAlignment="Top" />
                    <Label Content="Help:" HorizontalAlignment="Left" Margin="10,160,0,0" VerticalAlignment="Top" />
                    <Label Content="Starting Era:" HorizontalAlignment="Left" Margin="10,190,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Required Tech:" HorizontalAlignment="Left" Margin="10,220,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Portrait Index:" HorizontalAlignment="Left" Margin="10,250,0,0"
                           VerticalAlignment="Top" />
                    <TextBox x:Name="tbBuildingType" HorizontalAlignment="Left" Margin="125,10,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" CharacterCasing="Upper" />
                    <ComboBox x:Name="cbBuildingOverride" HorizontalAlignment="Left" Margin="125,40,0,0"
                              VerticalAlignment="Top" Width="120" SelectedIndex="38" Grid.Column="1" />
                    <TextBox x:Name="tbBuildingDesc" HorizontalAlignment="Left" Margin="125,70,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" />
                    <TextBox x:Name="tbBuildingPedia" HorizontalAlignment="Left" Margin="125,100,0,0"
                             TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" />
                    <TextBox x:Name="tbBuildingStrat" HorizontalAlignment="Left" Margin="125,130,0,0"
                             TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" />
                    <TextBox x:Name="tbBuildingHelp" HorizontalAlignment="Left" Margin="125,160,0,0"
                             TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" />
                    <ComboBox x:Name="cbBuildingStartEra" HorizontalAlignment="Left" Margin="125,190,0,0"
                              VerticalAlignment="Top" Width="120" SelectedIndex="4" Grid.Column="1" />
                    <ComboBox x:Name="cbBuildingReqTech" HorizontalAlignment="Left" Margin="125,220,0,0"
                              VerticalAlignment="Top" Width="120" SelectedIndex="20" Grid.Column="1" />
                    <x2:IntegerUpDown x:Name="intBuildingPortraitIndex" Margin="125,250,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="3" Increment="1" Minimum="0"
                                      Maximum="4" Width="120" AllowTextInput="False" Grid.Column="1" />
                </Grid>
            </TabItem>
            <TabItem Header="Units" IsEnabled="False">
                <Grid>
                    <Grid.Background>
                        <SolidColorBrush Color="#191919" Opacity="0.85" />
                    </Grid.Background>
                    <Label Content="Unit:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" />
                    <Label Content="Unit Type:" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" />
                    <TextBox x:Name="tbUnitType" HorizontalAlignment="Left" Margin="125,10,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" CharacterCasing="Upper" />
                    <ComboBox x:Name="cbUnitOverride" HorizontalAlignment="Left" Margin="125,40,0,0"
                              VerticalAlignment="Top" Width="120" SelectedIndex="0" Grid.Column="1" />
                </Grid>
            </TabItem>
            <TabItem Header="Icon Atlas" IsEnabled="False">
                <Grid>
                    <Grid.Background>
                        <SolidColorBrush Color="#191919" Opacity="0.85" />
                    </Grid.Background>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>