<Window x:Class="CivModTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:x2="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="Civilization V - Mod Tool"
        Height="550" Width="1000"
        FontFamily="Century Gothic"
        SizeToContent="Manual"
        ResizeMode="CanMinimize">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Resources/background.jpg" />
        </Grid.Background>
        <TabControl x:Name="TabControls" Margin="10,10,10,10" Background="Transparent">
            <TabItem Header="Civilization">
                <Grid>
                    <Grid.Background>
                        <SolidColorBrush Color="#191919" Opacity="0.85" />
                    </Grid.Background>
                    <TextBox x:Name="tbType" HorizontalAlignment="Left" Margin="130,10,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" LostFocus="TbType_OnLostFocus" CharacterCasing="Upper" />
                    <ComboBox x:Name="cbDerivative" HorizontalAlignment="Left" Margin="130,40,0,0"
                              VerticalAlignment="Top" Width="120" SelectedIndex="0" />
                    <TextBox HorizontalAlignment="Left" Margin="130,70,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" />
                    <TextBox HorizontalAlignment="Left" Margin="130,100,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" />
                    <TextBox HorizontalAlignment="Left" Margin="130,130,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" />
                    <TextBox HorizontalAlignment="Left" Margin="130,160,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" />
                    <TextBox HorizontalAlignment="Left" Margin="130,190,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" />
                    <ComboBox x:Name="cbArtStyle" HorizontalAlignment="Left" Margin="130,220,0,0"
                              VerticalAlignment="Top" Width="120" SelectedIndex="1" />
                    <x2:IntegerUpDown x:Name="intCivPortraitIndex" Margin="130,250,0,0" HorizontalAlignment="Left"
                                      VerticalAlignment="Top" FormatString="N0" Value="1" Increment="1" Minimum="0"
                                      Maximum="4" Width="120" AllowTextInput="False" />
                    <ComboBox x:Name="cbSoundtrack" HorizontalAlignment="Left" Margin="130,280,0,0"
                              VerticalAlignment="Top" Width="120" SelectedIndex="0" />
                    <TextBox HorizontalAlignment="Left" Margin="130,310,0,0" TextWrapping="Wrap"
                             VerticalAlignment="Top" Width="120" />
                    <Label Content="Type:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" />
                    <Label Content="Derivative:" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" />
                    <Label Content="Description:" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" />
                    <Label Content="Short Description:" HorizontalAlignment="Left" Margin="10,100,0,0"
                           VerticalAlignment="Top" Grid.ColumnSpan="2" />
                    <Label Content="Adjective:" HorizontalAlignment="Left" Margin="10,130,0,0" VerticalAlignment="Top" />
                    <Label Content="Civilopedia:" HorizontalAlignment="Left" Margin="10,160,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Civilopedia Tag:" HorizontalAlignment="Left" Margin="10,190,0,0"
                           VerticalAlignment="Top" Grid.ColumnSpan="2" />
                    <Label Content="Art Style:" HorizontalAlignment="Left" Margin="10,220,0,0" VerticalAlignment="Top" />
                    <Label Content="Portrait Index:" HorizontalAlignment="Left" Margin="10,250,0,0"
                           VerticalAlignment="Top" Grid.ColumnSpan="2" />
                    <Label Content="Soundtrack:" HorizontalAlignment="Left" Margin="10,280,0,0" VerticalAlignment="Top" />
                    <Label Content="Dawn of Man:" HorizontalAlignment="Left" Margin="10,310,0,0"
                           VerticalAlignment="Top" Grid.ColumnSpan="2" />
                    <Label Content="Primary Color:" HorizontalAlignment="Left" Margin="270,10,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Secondary Color:" HorizontalAlignment="Left" Margin="270,40,0,0"
                           VerticalAlignment="Top" />
                    <Label Content="Text Color:" HorizontalAlignment="Left" Margin="270,70,0,0" VerticalAlignment="Top" />
                    <x2:ColorPicker
                        x:Name="cpPrimaryColor"
                        Width="70"
                        Margin="400,10,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        ColorMode="ColorCanvas"
                        ShowRecentColors="True"
                        Foreground="Black" />
                    <x2:ColorPicker
                        x:Name="cpSecondaryColor"
                        Width="70"
                        Margin="400,40,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        ColorMode="ColorCanvas"
                        ShowRecentColors="True"
                        Foreground="Black" />
                    <x2:ColorPicker
                        x:Name="cpTextColor"
                        Width="70"
                        Margin="400,70,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        ColorMode="ColorCanvas"
                        ShowRecentColors="True"
                        Foreground="Black" />

                    <Button x:Name="btnGenerateXML" Content="Generate" HorizontalAlignment="Left"
                            Margin="10,350,0,0" VerticalAlignment="Top" Height="25" Width="100"
                            Click="btnGenerateXML_Click" />
                </Grid>
